{
  "expo": {
    "name": "depremkit",
    "slug": "depremkit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/welcome.png",
    "scheme": "depremkit",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "background-fetch",
          "remote-notification",
          "background-fetch",
          "remote-notification"
        ]
      },
      "bundleIdentifier": "com.anonymous.depremkit"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/welcome.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.depremkit",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/welcome.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/welcome.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/welcome.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
